{
  "compilerOptions": {
    "target": "es2016",
    "noEmit": false,
    "jsx": "preserve",
    "module": "commonjs",
    "rootDir": ".",
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "moduleResolution": "Node"
  },
  "exclude": ["/node_modules", "./dist"],
  "extends": "expo/tsconfig.base"
}
